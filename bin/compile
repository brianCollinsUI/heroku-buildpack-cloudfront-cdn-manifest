#!/usr/bin/env bash

build_dir=$1
cache_dir=$2
env_dir=$3
bin_dir=$(cd "$(dirname $0)"; pwd)

for import in "$(dirname ${bin_dir})/lib/"*; do
  source "$import"
done

mkdir -p public/assets
mkdir -p public/packs

download_file assets/manifest-$SOURCE_VERSION.json || \
  download_file assets/manifest-latest.json
